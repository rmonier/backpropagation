train_bp_synthetic = "julia --project=. network/bp.jl 'datasets/normalized/A1-synthetic.txt' '[9; 9; 5; 1]' sigmoid 0.1 0.5 1000"
train_bp_top10s = "julia --project=. network/bp.jl 'datasets/normalized/A1-top10s.txt' '[9; 9; 5; 1]' sigmoid 0.1 0.5 1000"
train_mlr = "julia --project=. network/mlr.jl"
evaluate = "python evaluation/evaluate.py"

